pre NUM_LOOPS = 100

pre LEN_DELAY = 65535
pre NUM_DELAY = 100

main:
  jsr writeout
  jsr delay
  jmp main


delay:
  for ldr a, 0; cmp a, NUM_DELAY; add a, 1; {
    for ldr b, 0; cmp b, LEN_DELAY; add b, 1; {

    }
  }
  rts

writeout:
  for ldr a, 0; cmp a, NUM_LOOPS; add a, 1; {
    str a, UART
  }
  rts
