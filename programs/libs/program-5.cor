
ldr a, 0
ldr b, 0
ldr c, 0

ldr a, 0b10001111
str a, R4000

ldr a, 0b00001111
str a, R4004

//ldr a, 2
//str a, R4003

ldr a, 0b1111
str a, R4015

ldr a, 0b00001111
str a, R400C

ldr a, 0

delay:
  add b, 1
  cmp b, 2000
  joc equal, go
  jmp delay
