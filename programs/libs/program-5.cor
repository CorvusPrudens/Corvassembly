go:

  ldr b, 0
  ldr c, 0
  sub a, 1

  add a, 100, d

  str d, R4002
  lsr d, 8, e
  str e, R4003

  sub a, 100, d
  str d, R4006
  lsr d, 8, e
  str e, R4007

  lsr a, 8, c

  str a, R400A
  str c, R400B

  str c, R400E

  jmp delay

stop:
  jmp stop
